<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.0.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_0_0.dtd">

<service-builder package-path="ca.cityofkingston.kingstonUtils">
	<namespace>cok_utils</namespace>
	
	<entity name="KingstonLayout" local-service="true" remote-service="false" table="kingstonLayout_db">
	
	<column name="kId" type="long" primary="true"></column>
	<column name="groupId" type="long" />

		<!-- Audit fields -->

		<column name="companyId" type="long" />
