<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="ca.cityofkingston.model.StaffPeople" />
	<class name="ca.cityofkingston.model.impl.StaffPeopleImpl" table="dir_people">
		<id column="id_" name="id" type="long">
			<generator class="assigned" />
		</id>
		<property column="group_" name="group" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="department" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="division" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="unit" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="title" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="last" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="first" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="name" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="phone" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="fax" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="email" type="com.liferay.portal.dao.orm.hibernate.StringType" />
		<property name="tstamp" type="org.hibernate.type.TimestampType" />
	</class>
</hibernate-mapping>